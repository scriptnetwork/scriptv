#channel0

#append new
function append_new {
    cat ../core0/us/etc/nodes.distr0 > /tmp/xxbbfg
    ush seeds >> /tmp/xxbbfg
    cat /tmp/xxbbfg | sort | uniq > ../core0/us/etc/nodes.distr0
    git diff ../core0/us/etc/nodes.distr0
}

function httpnodes {
    pushd ../core0/us > /dev/null
        bin/gen_http_nodes
    popd > /dev/null
}

append_new

pushd 



exit 0
