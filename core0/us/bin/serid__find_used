function find_serid {
	ack "serial_id\(\)" | grep "'.'" | sed "s/.*'\(.\)'.*/\1/"
        ack "serid\{"  | grep "'.'" | sed "s/.*'\(.\)'.*/\1/"
}

function find_defs {
	char=$1
	ack "serial_id\(\)" | grep "'$char'"
        ack "serid\{"  | grep "'$char'"
}

function output {
	pushd gov > /dev/null
		find_serid
	popd > /dev/null
	pushd wallet > /dev/null
		find_serid
	popd > /dev/null
}

function defs {
	pushd gov > /dev/null
		find_defs $1
	popd > /dev/null
	pushd wallet > /dev/null
		find_defs $1
	popd > /dev/null

}
output | sort | uniq -c


dups=$(output | sort | uniq -c | awk '{print $1" "$2}' | sort -r | grep -v "^1 .*" | awk '{print $2}')
for d in $dups; do
	defs $d
done


exit 0

