echo "STARTED"
date

hn=`hostname`
echo $hn
hostname -I
pubkeyh=`us-gov -home /home/gov/.us id | awk '{ print $NF }'`
echo $pubkeyh
echo ""


if [ "_$pubkeyh" == "_33QM4CJxJxkgUUp6HZcr4BJPh3Yq" ]; then
    echo "julio_apestigue" > /home/gov/id
    exit 0
fi
if [ "_$pubkeyh" == "_1VVjycieNhjjcUSLZiSUQnGJWxx" ]; then
    echo "noemi" > /home/gov/id
    exit 0
fi
if [ "_$pubkeyh" == "_2LCbMR7q5EyxakTdvaKXZzeb4kaH" ]; then
    echo "punisher" > /home/gov/id
    exit 0
fi
if [ "_$pubkeyh" == "_2mgamDYWV5ogAwL3wKhygR5BYreA" ]; then
    echo "pit3" > /home/gov/id
    exit 0
fi
if [ "_$pubkeyh" == "_2p5DodGLZg6wbYRnAHPn8CfARWpc" ]; then
    echo "satoshi" > /home/gov/id
    exit 0
fi
if [ "_$pubkeyh" == "_341CTowibNcJ9SXXhzfP9oYS21Yg" ]; then
    echo "pit2" > /home/gov/id
    exit 0
fi
if [ "_$pubkeyh" == "_3gaEPErNicpAyBxqrpMRoXcZFou9" ]; then
    echo "trucha" > /home/gov/id
    exit 0
fi
if [ "_$pubkeyh" == "_4UJGQuu7bqSjjNNym6o4ohs89Bea" ]; then
    echo "pit1" > /home/gov/id
    exit 0
fi
if [ "_$pubkeyh" == "_6vAWK14ssvmnDGYLxDC4sAyrR9t" ]; then
    echo "pit5" > /home/gov/id
    exit 0
fi
if [ "_$pubkeyh" == "_DvJNG79sxFswxyPJhm2L4hrLjhv" ]; then
    echo "pi1" > /home/gov/id
    exit 0
fi
if [ "_$pubkeyh" == "_GutkKVYN1Q6n75XHdhtX4pvCWFi" ]; then
    echo "darren" > /home/gov/id
    exit 0
fi
if [ "_$pubkeyh" == "_CoJHqw1AgkcApr2VziZc3CaheoJ" ]; then
    echo "pit4" > /home/gov/id
    exit 0
fi

date
echo "FINISHED"

